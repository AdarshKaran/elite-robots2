<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="elite">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <xacro:arg name="use_gazebo" default="false"/>
    <xacro:arg name="arm" default="ec66"/>

    <!-- Import ec66 urdf file with use_gazebo set to "false" and arm argument-->
    <xacro:include filename="$(find elite_description)/urdf/elite_description.urdf.xacro"/>
</robot>
